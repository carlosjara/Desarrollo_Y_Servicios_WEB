<div class="container-fluid">
<nav class="navbar navbar-inverse">
    <div class="navbar-header">
        <p class="navbar-text" href="#">{{Titulo}}</p>
    </div>
</nav>
<div class="row">
    <div class="col-xs-12 col-md-12">
    	<form class="form-horizontal">
    	<hr>
    	{{#each enlaces}}
            <div class="panel panel-primary">
                  <div class="panel-body">
                    <strong style="text-transform:uppercase;"">{{nombre}}</strong>
                    <div class="btn-group btn-group-justified" role="group">
                        <div class="btn-group" role="group">
                            <button type="button" class="btn btn-success col-md-6" onclick="window.location.href='/delete?nombre={{nombre}}'">
                            <span class="glyphicon glyphicon-remove"></span> Eliminar
                            </button>
                        </div>
                        <div class="btn-group" role="group">
                            <button type="button" class="btn btn-success col-md-6" onclick="window.location.href='/edit?nombre={{nombre}}&url={{link}}'">
                            <span class="glyphicon glyphicon-ok"></span> Editar
                            </button>
                        </div>
                    </div>
                  </div>
                  <div class="panel-footer"><a href="{{link}}">{{link}}</a>
                  </div>

            </div>
    	{{/each}}
    	</form>
    </div>
</div>
<div class="btn-group btn-group-justified" role="group">
    <div class="btn-group col-md-4" role="group">
        <button class="btn btn-info" onclick="window.location.href='/new'">Agregar</button>
    </div>
</div>
<br>
</div>